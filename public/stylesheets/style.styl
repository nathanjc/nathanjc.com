
body 
  background #f5f5f5 url('/images/noise.png')
  font-family "Source Sans Pro", sans-serif
  color #222
  padding-top 4em
  padding-bottom 5em

h1, h2, h3, h4, h5
  font-family "Source Sans Pro", sans-serif
  color #222

h6
  font-family "Ubuntu Mono Regular"
  color #c3c3c3

p 
  font-size 1em
  font-family "Source Sans Pro", sans-serif

ul
  margin-top 32px
  font-size 32px

li
  margin-bottom 16px
  margin-top 16px
  font-size 16px

pre 
  border 1px solid #f5f5f5 
  background #fff url(/images/noise.png) 
  font-family "Ubuntu Mono", monospace; margin-bottom: 1em

code 
  border-radius: 5px

.project
  

.buttonlink
  text-decoration none

.projectimg
  max-width 300px

.morelink
  padding 0
  margin 0
  margin-bottom 16px
  float right

.profile 
  padding 1px

.navbar h5
  display inline-block

.navbar a
  color #222
  margin-right 2em

.project
  padding-top 16px

